<div class="panel panel-default">
  <div class="panel-heading">
    <h3>QA for <%= @ticket.ticket_reference %></h3>
  </div>
  <div class="panel-body">
    <% @qas.each do |qa| %>
      <% form_for(qa) do |f| %>
        <%= render 'criteria', f: f %>
      <% end %>
    <% end %>
  </div>
  <div class="panel-footer">
    <%= render "back" %>
  </div>
</div>

