<% if @user.tickets.where('tickets.date BETWEEN ? AND ?', @date_from, @date_to).count == 0 %>
  <%= render "warning", message: "User #{@user.name} #{@user.last_name} has No QA items for this date range" %>
<% elsif @current_user.agent_id == @user.id %>
  <%= render "agent_view" %>
<% else %>

  <% if params[:highlight_qa].present? %>
    <%= render 'info', message: "The QA you selected should be highlighted below" %>
  <% end %>

  <%= render "qa_list", date_from: @date_from, date_to: @date_to %>

<% end %>
