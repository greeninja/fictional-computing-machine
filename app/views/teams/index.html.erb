<div class="row">
  <%= render "daterange" %><br />
</div>
<div class="row">
  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="text-center text-muted">
          Teams Ticks & Crosses
        </h3>
      </div>
      <div class="panel-body">
        <%= render "teams" %>
        <% unless @current_user.team_id? %>
        <h3 class="text-center">Overview</h3>
          <ul class="nav nav-tabs">
            <li class="active"><a data-toggle="tab" href="#overview">Overview</a></li>
            <li><a data-toggle="tab" href="#crosses">Crosses</a></li>
            <li><a data-toggle="tab" href="#ticks">Ticks</a></li>
          </ul>
          <div class="tab-content">
            <div id="overview" class="tab-pane active">
              <%= render 'overview' %>
            </div>
            <div id="crosses" class="tab-pane">
              <%= render "cross_overview" %>
            </div>
            <div id="ticks" class="tab-pane">
              <%= render "ticks_overview" %>
            </div>
          </div>
        <% end %>
        </div>
      <div class="panel-footer">
      </div>
    </div>
  </div>
</div>
