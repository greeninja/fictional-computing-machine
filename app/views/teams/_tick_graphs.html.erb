<div class="row">
  <% @tick_types.sorted.each do |t| %>
    <div class="col-md-6">
      <h3 class="text-muted text-center">
      <%= t.name %>
      </h3>
      <%= bar_chart graph.joins(:ticks).where("ticks.tick_type_id = #{t.id}").group(:name).count, :library => {:colors => ["green"]} %>
    </div>
  <% end %>
</div>
