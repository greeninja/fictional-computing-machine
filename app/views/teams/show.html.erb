<div class="row">
  <%= render "daterange" %>
</div>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3>Team: <%= @team.name %></h3>
  </div>
  <div class="panel-body">
  <blockquote>
    <p><%= @team.description %></p>
    <small><%= @team.name %></small>
  </blockquote>
  </div>
  <ul class="nav nav-tabs">
    <li class="active"><a data-toggle="tab" href="#overview">Overview</a></li>
    <li><a data-toggle="tab" href="#crosses">Crosses</a></li>
    <li><a data-toggle="tab" href="#ticks">Ticks</a></li>
  </ul>
  <div class="tab-content">
    <div id="overview" class="tab-pane active">
      <%= render 'graphs', graph: Agent.where(team_id: @team.id), team_name: @team.name %>
    </div>
    <div id="crosses" class="tab-pane">
      <%# render "warning", message: 'This section is still under development. See Github for more information.' %>
      <%= render "cross_graphs", graph: Agent.where(team_id: @team.id) %>
    </div>
    <div id="ticks" class="tab-pane">
      <%# render "warning", message: 'This section is still under development. See Github for more information.' %>
      <%= render "tick_graphs", graph: Agent.where(team_id: @team.id) %>
    </div>
  </div>
  <div class="panel-footer">
    <%= render "back" %>
  </div>
</div>
