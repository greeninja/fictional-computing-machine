<div class="panel panel-default">
  <div class="panel-heading">
    <h3>Team: <%= @team.name %></h3>
  </div>
  <div class="panel-body">
  
  <blockquote>
    <p><%= @team.description %></p>
    <small><%= @team.name %></small>
  </blockquote>
  </div>
  <ul class="nav nav-tabs">
    <li class="active"><a data-toggle="tab" href="#overview">Overview</a></li>
    <li><a data-toggle="tab" href="#rats">Rats</a></li>
    <li><a data-toggle="tab" href="#icks">Ticks</a></li>
  </ul>
  <div class="tab-content">
    <div id="overview" class="tab-pane active">
      <%= render 'graphs', graph: User.where(team_id: @team.id), team_name: @team.name %>
    </div>
    <div id="rats" class="tab-pane">
      <%= render "warning", message: 'This section is still under development. See Github for more information.' %>
    </div>
    <div id="ticks" class="tab-pane">
      <%= render "warning", message: 'This section is still under development. See Github for more information.' %>
    </div>
  </div>
  <div class="panel-footer">
    <%= render "back" %>
  </div>
</div>
