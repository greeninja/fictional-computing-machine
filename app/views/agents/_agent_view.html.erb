<div class="row">
</div>

<div class="panel panel-default">
    <div class="panel-heading">
      <h4 class="text-center text-muted">Ticks and Crosses</h4>
    </div>
    <div class="panel-body">
      <div class="row">
        <%= render "daterange" %>
      </div>

    <div class="row">
      <div class="col-md-10 col-md-offset-1 col1">
        <div class="panel panel-default">
          <div class="panel-body">
            <h4 class="text-center text-muted"><%= ("my ticks and crosses").titleize %></h4>
            <div class="progress">
              <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="min-width: 2em; width: <%= agent_get_tick_percentages(@user) %>%;">
                <%= @user_ticks.nil? ? "0" : @user_ticks.count %>
              </div>
            </div>
            <div class="progress">
              <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="min-width: 2em; width: <%= agent_get_cross_percentages(@user) %>%;">
                <%= @user_crosses.nil? ? "0" : @user_crosses.count %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>


<% arr = ["ticks", "crosses"] %>
<% arr.each do |type| %>
<div class="col-md-6">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h4 class="text-center text-muted">My <%= type.titleize %></h4>
    </div>
    <div class="panel-body">
      <div class="col-sm-12">
        <%= render "agent_#{ type }", just_agent: true %>
      </div>
    </div>
    <div class="panel-footer">
    </div>
  </div>
</div>
<div class="col-md-6">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h4 class="text-center text-muted">Team <%= type.titleize %></h4>
    </div>
    <div class="panel-body">
      <div class="col-sm-12">
        <%= render "team_#{type}", just_agent: true %>
      </div>
    </div>
    <div class="panel-footer">
    </div>
  </div>
</div>
<% end %>

  </div>
  <div class="panel-footer">
  </div>
