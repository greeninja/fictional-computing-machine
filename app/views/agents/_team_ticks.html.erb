<% if @team_ticks.nil? %>
  <%= render "success", message: "Team has no Ticks" %>
<% else %>
  <% @tick_types.each do |type| %>
    <div class="col-sm-4">
      <div class="panel panel-default">
        <div class="panel-body">
          <h4 class="text-center text-muted"><%= type.name.titleize %></h4>
          <h3 class="text-center"><span class="label label-success"><%= @team_ticks.where(:tick_type_id => type.id).count %></span></h3>
        </div>
      </div>
    </div>
  <% end %>
<% end %>
