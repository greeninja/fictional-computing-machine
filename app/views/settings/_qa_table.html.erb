<h3 class="text-center text-muted">QA Settings</h3>
<table class="table table-hover table-condensed table-bordered table-responsive">
  <thead>
    <tr>
      <th>Team</th>
      <th>QA Criteria</th>
      <th>Description</th>
      <th>Out Of</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tr>
    <td><%= qa.team.name %></td>
    <td><%= qa.qa %></td>
    <td><%= qa.description %></td>
    <td><%= qa.out_of %></td>
    <td>
      <div class="btn-group">
        <%= link_to "Edit", edit_setting_qa_setting_path(@setting, qa), :class => "btn btn-sm btn-default" %>
          <button data-toggle="dropdown" class="btn btn-sm btn-default dropdown-toggle">
            <span class="caret"></span>
          </button>
          <ul class="dropdown-menu">
          <li>
            <%= link_to "Delete", qa, method: :delete, data: { confirm: "Are you sure?" } %>
          </li>
        </ul>
      </div>
    </td>
  </tr>
</table>
