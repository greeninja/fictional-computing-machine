<div class="media well">
  <%= image_tag("tick.png", :alt => "ticks", :class => "pull-left") %>
  <div class="media-body">
    <h4 class="text-center text-muted">Ticks</h4>
    <% if defined?(just_agent) %>
      <% get_tick_percentages(@agent) %>
        <p><strong><font color="green"><%= @tnumber_of %> Ticks</font></strong> making up <strong><%= @tpercent %>% </strong> of your teams total.<br />
        <div class="progress">
          <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="min-width: 2em; width: <%= @tpercent %>%;">
            <%= @tnumber_of %>
          </div>
    <% else %>
      <p>Total Ticks between <%= @date_from.to_s(:long) %> and <%= @date_to.to_s(:long) %><br />
      <div class="progress">
        <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="min-width: 2em; width: 100%;">
          <%= @team_ticks %>
        </div>
    <% end %>
    </div>
    <%# link_to("Show", {:controller => 'agents', :action => "show", :id => @current_user.agent_id}, :class => "btn btn-sm btn-default pull-right") %>
  </div>
</div>
