<div class="col-md-6">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title text-center text-muted">Team Overview</h3>
    </div>
    <div class="panel-body">
      <% if @team.ticks_enabled? %>
        <div class="col-sm-12">
          <%= render "ticks" %>
        </div>
      <% end %>
      <% if @team.crosses_enabled? %>
        <div class="col-sm-12">
          <%= render "crosses" %>
        </div>
      <% end %>
      <% if @team.qa_enabled? %>
        <div class="col-sm-12">
          <%= render "qa" %>
        </div>
      <% end %>
      <div class="col-sm-12">
        <%= render "surveys" %>
      </div>
      <div class="col-sm-12">
        <%= render "sales" %>
      </div>
    </div>
    <div class="panel-footer">
    </div>
  </div>
</div>
